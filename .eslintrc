{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [2, "never"],
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0
  },
  "env" : {
      "browser": true,
      "node": true 
  }
}